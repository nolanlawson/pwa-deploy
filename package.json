{
  "name": "pwa-deploy",
  "version": "0.1.1",
  "description": "CLI to deploy a Progressive Web App to an attached Android device or emulator",
  "main": "lib/bin.js",
  "bin": {
    "pwa-deploy": "lib/bin.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "bash bin/build.sh",
    "test": "semistandard lib/*js"
  },
  "keywords": [
    "apk",
    "pwa",
    "android"
  ],
  "author": "Nolan Lawson <nolan@nolanlawson.com>",
  "license": "Apache-2.0",
  "files": [
    "lib",
    "android/app/build/outputs/apk/app-debug.apk",
    "exec"
  ],
  "dependencies": {
    "bluebird": "^3.4.0",
    "child-process-promise": "^2.0.2",
    "denodeify": "^1.2.1",
    "mkdirp": "^0.5.1",
    "temp": "^0.8.3",
    "which": "^1.2.9",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "semistandard": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nolanlawson/pwa-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/nolanlawson/pwa-deploy/issues"
  },
  "homepage": "https://github.com/nolanlawson/pwa-deploy#readme"
}
